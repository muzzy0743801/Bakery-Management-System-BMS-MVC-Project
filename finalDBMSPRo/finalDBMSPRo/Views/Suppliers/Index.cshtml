@model IEnumerable<finalDBMSPRo.Models.Supplier>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.SupId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SupPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SupAddress)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <th>
        @Html.DisplayFor(modelItem => item.SupId)
    </th>
    <td>
        @Html.DisplayFor(modelItem => item.FirstName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SupPhone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SupAddress)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.SupId }) |
        @Html.ActionLink("Details", "Details", new { id = item.SupId }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.SupId })
    </td>
</tr>
}

</table>
